# Environment
NODE_ENV=development

# Database (required)
# PostgreSQL connection string - local or hosted (e.g., Vercel Postgres, Neon, Supabase)
POSTGRES_URL=

# Authentication (required)
# Generate with: openssl rand -base64 32
# Note: In development, a default secret is used if not set
AUTH_SECRET=

# v0 API (required)
# Get your API key from: https://v0.app/chat/settings/keys
V0_API_KEY=

# Optional: Custom v0 API URL (leave empty for default)
V0_API_URL=
